  "dependencies": {
    "@naartti/react-localize-redux": "^3.5.4",
    "@sentry/browser": "^5.6.3",
    "axios": "^0.18.1",
    "babel-polyfill": "^6.26.0",
    "better-react-mathjax": "^2.0.2",
    "papaparse": "^4.6.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-csv": "^2.2.2",
