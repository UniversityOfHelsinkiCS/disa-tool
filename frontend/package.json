  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.1",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.2.0",
    "babel-preset-env": "^1.7.0",
